<!doctype html>

<html>
<head>
	<title>PickMe</title>	
	<meta charset="utf-8">
	<meta name="viewport" content="width=device-width, initial-scale=1.0">
	<link rel="stylesheet" href="css/bootstrap.css"/>  
</head>

<body style="padding-top: 70px;padding-bottom: 30px;margin:30px">
	<div class="container">
		<div class="row">
			<div class="col-md-6 col-md-offset-3">
				<div align="center">
					<form id="addPhoneNumberForm" role="form">
						
						<h3>Input Phone Number</h3>

						<input type="txt" class="form-control" placeholder="phone number" name="phoneNumber">

						<button id="phonebutton" type="submit" class="btn btn-primary" style="margin:5px">Submit</button>

					</form>
				</div>
			</div>
		</div>
	</div>

